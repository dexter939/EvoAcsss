{
  "cli": {
    "version": ">= 10.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ACS_API_URL": "",
        "DEBUG": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "env": {
        "ACS_API_URL": "",
        "DEBUG": "false"
      }
    },
    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "env": {
        "ACS_API_URL": "",
        "DEBUG": "false"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "",
        "ascAppId": "",
        "appleTeamId": ""
      },
      "android": {
        "serviceAccountKeyPath": "./google-services.json",
        "track": "internal"
      }
    }
  }
}
